<table class="table table-striped">  
    <tr class="table-dark">
    <th>Name</th>
    <th>Country</th>
    <th>Address</th> 
    <th>Phone</th> 
    <th>Website</th> 
    <th>Venue</th> 
    <th>Actions</th> 
  </tr>

{{#each items as |item|}}
  <tr>
    <td>
      <a href="http://localhost:8084/equipo/{{item.id}}/ver">{{item.shortName}}</a>
    </td>
    <td>{{item.area.name}}</td>
    <td>{{item.address}}</td>
    <td>{{item.phone}}</td>
    <td>{{item.website}}</td>
    <td>{{item.venue}}</td>
    <td>
      <a href="http://localhost:8084/equipo/{{item.id}}/ver">Ver</a>
      <a href="http://localhost:8084/equipo/{{item.id}}/actualizar">Actualizar</a>
      <a href="http://localhost:8084/equipo/{{item.id}}/eliminar">Eliminar</a>
    </td> 
  </tr>
{{/each}}
</table>

